alias gb='git branch'
alias gba='git branch -a'
alias gc='git commit -av'
alias gd='git diff | mvim'
alias gpu='git pull'
alias gp='git push'
alias gpm='git push origin master'
alias gst='git status'
alias r='fc -s'
alias ls='ls -G'
alias rom='rebaseAllBranchesToMaster'
alias ram='git rebase origin/master'
alias gl="git log --graph --pretty=format:'%Cred%h%Creset %C(cyan)%an%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"
alias lebs='lebowski-spec $1 -f nested -f h:~/reports/lebowski_run.html'
